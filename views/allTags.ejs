<%- include("partials/stackHeader"); -%>
<div>
  <div>
    <div class="jumbotron">
      <h1 class="display-3">All Tags</h1>
			<form method="POST" action="/tags">
				<div class="form-group row userSearch mt-1">
					<div class="col-xs-3">
						<div class="input-group">
							<input type="text" class="form-control" name="tagname" placeholder="🔍Tagname">
						</div>
					</div>
				</div>
			</form>
    </div>
  </div>
</div>




























<div>
	<div>
		<div class="row">
		<%tags.forEach(function(item){%>
		<div class="col-md-3 col-sm-6">
			<div class="thumbnail">

			</div>
			<div class="caption ">
				<span class="border border-light">
					<a href="/tags/<%=item._id%>"><%=item.name%></a>
				</span>

			</div>

		</div>

		<% });%>
			</div>
	</div>
</div>
<%- include("partials/stackFooter"); -%>
